    "roarr": "^2.14.6"
  },
  "description": "TMDb SDK.",
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/node": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/plugin-transform-flow-comments": "^7.7.4",
